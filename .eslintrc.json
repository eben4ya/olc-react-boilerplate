{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard-with-typescript", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["react"],
  "rules": {
    "@typescript-eslint/quotes": "off", // Menonaktifkan aturan quotes
    "@typescript-eslint/semi": "off" // Menonaktifkan aturan semi
  }
}
